{
  "name": "hardhat-circom",
  "version": "0.0.1",
  "description": "(unofficial) Hardhat plugin for iden3 circom and snarkjs build process",
  "repository": "github:projectsophon/hardhat-circom",
  "author": "Jacob Rosenthal",
  "license": "GPL-3.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "prepare": "tsc -p . && cpy ./src/Verifier.sol.template dist",
    "lint": "exit 0",
    "test": "mocha --exit --recursive 'test/**/*.test.ts'",
    "format": "prettier --write .",
    "build": "tsc",
    "watch": "tsc -w"
  },
  "files": [
    "dist/",
    "src/",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "circom": "0.5.45",
    "circomlib": "^0.2.4",
    "snarkjs": "^0.3.60"
  },
  "peerDependencies": {
    "hardhat": "^2.0.0"
  },
  "devDependencies": {
    "@projectsophon/prettier-config": "0.0.0",
    "@projectsophon/tsconfig": "0.0.0",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.22",
    "chai": "^4.2.0",
    "cpy-cli": "3.1.1",
    "hardhat": "^2.0.0",
    "mocha": "^8.0.1",
    "prettier": "^2.2.1",
    "ts-node": "^8.1.0",
    "typescript": "4.2.x"
  },
  "prettier": "@projectsophon/prettier-config",
  "keywords": [
    "snarkjs",
    "iden3",
    "circom",
    "hardhat",
    "hardhat-plugin"
  ]
}
