{
  "name": "hardhat-circom",
  "version": "1.0.1",
  "description": "Hardhat plugin to integrate Circom and SnarkJS into your build process.",
  "author": "Jacob Rosenthal <jacobrosenthal@gmail.com>",
  "repository": "projectsophon/hardhat-circom",
  "license": "GPL-3.0",
  "engines": {
    "node": ">=14"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "src/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prepare": "tsc -p . && cpy ./src/Verifier.sol.template dist",
    "lint": "eslint .",
    "test": "mocha --exit --recursive 'test/**/*.test.ts'",
    "format": "prettier --write .",
    "build": "tsc",
    "watch": "tsc -w"
  },
  "dependencies": {
    "circom": "0.5.45",
    "circomlib": "^0.2.4",
    "debug": "^4.3.1",
    "snarkjs": "^0.3.60"
  },
  "peerDependencies": {
    "hardhat": "^2.0.0"
  },
  "devDependencies": {
    "@projectsophon/eslint-config": "0.0.2",
    "@projectsophon/prettier-config": "0.0.0",
    "@projectsophon/tsconfig": "0.0.0",
    "@types/chai": "^4.2.14",
    "@types/debug": "^4.1.5",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.22",
    "chai": "^4.2.0",
    "cpy-cli": "3.1.1",
    "eslint": "^7.25.0",
    "hardhat": "^2.0.0",
    "mocha": "^8.0.1",
    "prettier": "^2.2.1",
    "ts-node": "^8.1.0",
    "typescript": "4.2.x"
  },
  "prettier": "@projectsophon/prettier-config",
  "eslintConfig": {
    "extends": "@projectsophon/eslint-config"
  },
  "keywords": [
    "snarkjs",
    "iden3",
    "circom",
    "hardhat",
    "hardhat-plugin"
  ]
}
